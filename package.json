{
  "name": "alphaverse",
  "version": "1.0.4",
  "description": "Alphaverse: The Casino Master of the Multiverse. Roll the dice and dominate the digital cosmos with our state-of-the-art betting bot!",
  "type": "module",
  "scripts": {
    "server": "node server/index.js",
    "client": "node --max-old-space-size=800 --expose-gc client/index.js",
    "client:lowmem": "node --max-old-space-size=400 --expose-gc client/index.js",
    "client:highmem": "node --max-old-space-size=1024 --expose-gc client/index.js",
    "getIPs": "node server/getIPs.js",
    "2faAPI": "node server/2faServer.js",
    "apiServer": "node server/2faServer.js",
    "postinstall": "node client/fix-tr46.js",
    "dicebot": "node --max-old-space-size=800 --expose-gc client/baccaratChopTie_v2.1.js",
    "dicebot:lowmem": "node --max-old-space-size=400 --expose-gc client/baccaratChopTie_v2.1.js",
    "checkCookie": "node client/checkCookie.js",
    "checkCookie:debug": "node --inspect client/checkCookie.js",
    "start": "npm run client",
    "restart:client": "pkill -f 'node client/index.js' && sleep 3 && npm run client",
    "status": "ps aux | grep -E 'node (client/index.js|client/baccaratChopTie)' | grep -v grep"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "discord.js": "^14.17.3",
    "express": "^4.21.1",
    "fs": "^0.0.1-security",
    "got": "^14.4.5",
    "https-proxy-agent": "^7.0.5",
    "javascript-obfuscator": "^4.1.1",
    "moment": "^2.30.1",
    "node-2fa": "^2.0.3",
    "node-fetch": "^3.3.2",
    "node-fetch-retry-timeout": "^1.2.1",
    "pug": "^3.0.3",
    "punycode": "^2.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tr46": "^5.0.0",
    "tronweb": "^6.0.0",
    "upgrade": "^1.1.0",
    "uuid": "^11.0.3",
    "whatwg-url": "^14.0.0",
    "ws": "^8.18.0"
  }
}
